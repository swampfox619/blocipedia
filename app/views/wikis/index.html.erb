<h1>Wiki Master Page</h1>
<div class = "row">
    <div class = "col-sm-8">
        <% @wikis.each do |wiki| %>
            <div class="media">
                <div class="media-body">
                    <h4 class="media-heading">
                        <p><%= link_to wiki.title, wiki %></p>
                    </h4>
                </div>
            </div>
        <% end %>
    </div>
    <div class = "col-sm-2">
        <%= link_to "New Wiki", new_wiki_path, class: 'btn btn-primary' %>
    </div>
</div>
